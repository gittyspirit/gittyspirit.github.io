  <entry>
    <title>再见，过去的美好生活</title>
    <link href="https://www.spiritfr.eu.org/2024/05/05/%E5%86%8D%E8%A7%81%EF%BC%8C%E8%BF%87%E5%8E%BB%E7%9A%84%E7%BE%8E%E5%A5%BD%E7%94%9F%E6%B4%BB/"/>
    <id>https://www.spiritfr.eu.org/2024/05/05/%E5%86%8D%E8%A7%81%EF%BC%8C%E8%BF%87%E5%8E%BB%E7%9A%84%E7%BE%8E%E5%A5%BD%E7%94%9F%E6%B4%BB/</id>
